#!/usr/bin/ruby

require 'cgi'

cgi = CGI.new('html4')

cgi.out do
  cgi.html do
    cgi.head do
      cgi.title { "Overwatch" }
      cgi.link({'rel' => 'stylesheet', 'href' => 'style.css'})
      cgi.script({'type' => 'text/javascript', 'src' => 'script.js'})
    end
    cgi.body do
      cgi.div { "Hello World" }
    end
  end
end
